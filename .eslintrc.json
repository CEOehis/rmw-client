{
    "extends": "airbnb-base",
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "document": false
    },
    "rules": {
    "linebreak-style": ["warn", "windows"],
    "arrow-body-style": ["error", "always"],
    "strict": ["error", "never"],
    "object-curly-newline": ["error", { "consistent": true }],
    "function-paren-newline": ["warn", "consistent"]
  }
}